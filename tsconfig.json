{
   "compilerOptions": {
       "target": "es2016",
       "module": "commonjs",
       "jsx": "react-native",
       "outDir": "build",
       "rootDir": "src",
       "allowSyntheticDefaultImports": true,
       "noImplicitAny": false,
       "experimentalDecorators": true,
       "preserveConstEnums": true,
       "allowJs": true,
       "sourceMap": true,
       "forceConsistentCasingInFileNames": true,
       "noImplicitReturns": true,
       "noImplicitThis": true,
       "strictNullChecks": true,
       "suppressImplicitAnyIndexErrors": true,
       "noUnusedLocals": true,
       "skipLibCheck": true,
       "moduleResolution": "node"
   },
   "exclude": [
       "App.js",
       "App.test.js",
       "build",
       "node_modules"
   ],
   "compileOnSave": false
}